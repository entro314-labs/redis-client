{
  "name": "@entro314labs/redis-client",
  "version": "1.0.0",
  "description": "Redis client wrapper with caching utilities for Entrolytics",
  "author": "Entrolytics <hey@entrolytics.click>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/entro314-labs/redis-client.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/entro314-labs/redis-client",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs --watch",
    "lint": "biome check .",
    "format": "biome format --write .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "debug": "^4.4.3",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.1",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-esbuild": "^6.2.1",
    "typescript": "^5.9.3",
    "vitest": "latest"
  },
  "engines": {
    "node": ">=22.x"
  },
  "keywords": [
    "redis",
    "cache",
    "rate-limiting",
    "analytics",
    "entrolytics"
  ]
}
